<Configuration xmlns="http://itude.com/schemas/MB/2.0">
	<Model>
        <Documents>
            <Document autoCreate="TRUE" name="HomePageContentDocument" dataManager="MBMemoryDataHandler">
				<Element name="testgroup">
					<Element name="testitem">
						<Element name="testelement">
							<Attribute name="text()" type="string"/>
						</Element>
					</Element>
				</Element>
            </Document>
            <Document name="LoginUserDocument" dataManager="MBMemoryDataHandler" autoCreate="TRUE">
				<Element name="LoginUser" minOccurs="1">
					<Attribute name="username" type="string" defaultValue="" />
					<Attribute name="password" type="string" defaultValue="" />
				</Element>
			</Document>
        </Documents>
    </Model>
</Configuration>