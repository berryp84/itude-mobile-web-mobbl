<Configuration xmlns="http://itude.com/schemas/MB/2.0">
<Model>
	<Documents>
		<Document name="MBGenericResponse" dataManager="BinckDataHandler" autoCreate="TRUE">
			<Element name="Response" minOccurs="1">
				<Attribute name="title" type="string"/>
				<Attribute name="body" type="string"/>
				<Attribute name="error" type="string"/>
				<Attribute name="outcomeName" type="string"/>
			</Element>
		</Document>
		<Document name="SessionState" dataManager="SessionDataHandler" autoCreate="TRUE">
			<Element name="Session" minOccurs="1">
				<Attribute name="loggedIn" type="string" defaultValue="false"/>
			</Element>
		</Document>
	</Documents>
</Model>
</Configuration>